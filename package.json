{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run build:clean",
    "build:browser:prod": "cross-env NODE_ENV=production webpack --mode production --config kit/webpack/webpack.browser.prod.js -p --progress",
    "build:browser:dev": "cross-env NODE_ENV=development webpack --mode development --config kit/webpack/webpack.browser.dev.js -d --progress --debug --run-dev --log-level debug",
    "build:server:prod": "cross-env NODE_ENV=production webpack --mode development --config kit/webpack/webpack.server.prod.js -p --progress",
    "build:server:dev": "cross-env NODE_ENV=development webpack --mode development --config kit/webpack/webpack.server.dev.js -d --progress --debug --run-dev --log-level debug",
    "build:clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "clean": "npm run test:clean && npm run build:clean",
    "start:prod": "cross-env NODE_ENV=production node server",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "eslint:fix": "eslint -- . --ignore-path .gitignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
